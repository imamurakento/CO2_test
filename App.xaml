<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:CO2"
             xmlns:material="http://schemas.enisn-projects.io/dotnet/maui/uraniumui/material"
             xmlns:helper="clr-namespace:CO2.At.Src.bDomain.Helpers"
             x:Class="CO2.App" AutomationProperties.IsInAccessibleTree="False">
    <Application.Resources>

        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="appColors" Source="Resources/Styles/TrustColors.xaml" />
                <ResourceDictionary x:Name="appStyles" Source="Resources/Styles/TrustStyles.xaml" />
                <material:StyleResource />
            </ResourceDictionary.MergedDictionaries>

            <helper:TextChangedEventArgsConverter x:Key="TextChangedEventArgsConverter" />
            <helper:UnfocusedEventArgsConverter x:Key="UnfocusedEventArgsConverter" />
            <helper:CompletedEventArgsConverter x:Key="CompletedEventArgsConverter" />
        </ResourceDictionary>

    

    </Application.Resources>
    

    
</Application>
